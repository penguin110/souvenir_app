



<div class = "col-xs-10">
    <% @prefectures.pluck(:region_id).uniq.each do |pre|%>
        <% if pre == 1 %>
        <h3><%= "北海道・東北地方" %></h3>
            <% @pre1.each do |pre| %>
            <%= link_to pre.name, sou_index_path(pre.id) %>
            <% end %>
        <% elsif pre == 2 %>
        <h3><%= "関東地方" %></h3>
        <% @pre2.each do |pre| %>
            <%= link_to pre.name, sou_index_path(pre.id) %>
            <% end %>
        <% elsif pre == 3 %>
        <h3><%= "中部地方" %></h3>
        <% @pre3.each do |pre| %>
            <%= link_to pre.name, sou_index_path(pre.id) %>
            <% end %>
        <% elsif pre == 4 %>
        <h3><%= "近畿地方" %></h3>
        <% @pre4.each do |pre| %>
            <%= link_to pre.name, sou_index_path(pre.id) %>
            <% end %>
        <% elsif pre == 5 %>
        <h3><%= "中国地方" %></h3>
        <% @pre5.each do |pre| %>
            <%= link_to pre.name, sou_index_path(pre.id) %>
            <% end %>
        <% elsif pre == 6 %>
        <h3><%= "四国地方" %></h3>
        <% @pre6.each do |pre| %>
            <%= link_to pre.name, sou_index_path(pre.id) %>
            <% end %>
        <% elsif pre == 7 %>
        <h3><%= "九州地方" %></h3>
        <% @pre7.each do |pre| %>
            <%= link_to pre.name, sou_index_path(pre.id) %>
            <% end %>
        <% end %>
    <% end %>
